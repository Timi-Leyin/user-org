generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model user {
  id   Int    @unique @default(autoincrement())
  uuid String @id @unique @default(cuid())

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
